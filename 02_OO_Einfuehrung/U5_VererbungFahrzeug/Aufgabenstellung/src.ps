(   3|class Fahrzeug {) s
(   4|  ) s
(   5|  private double mPosX;) s
(   6|  private double mPosY;) s
(   7|  ) s
(   8|  ) s
(   9|  public Fahrzeug\(double pXinit, double pYinit\) {) s
(  10|    mPosX = pXinit;) s
(  11|    mPosY = pYinit;) s
(  12|  }) s
(  13|  ) s
(  14|  ) s
(  15|  public double fahren\(double pX, double pY\) {) s
(  16|    System.out.print\("Fahrzeug.fahren\(\): "\);) s
(  17|    double xDiff = mPosX - pX;) s
(  18|    double yDiff = mPosY - pY;) s
(  19|    double km = Math.sqrt\(xDiff * xDiff + yDiff * yDiff\);) s
(  20|    mPosX = pX;) s
(  21|    mPosY = pY;) s
(  22|    System.out.println\(km\);) s
(  23|    return km;) s
(  24|  }) s
(  25|  ) s
(  26|  ) s
(  27|  protected void printInfos\(String pPrefix\) {) s
(  28|    System.out.print\(pPrefix + ": Koord = " + mPosX + "/" + mPosY\);) s
(  29|  }) s
(  30|  ) s
(  31|  ) s
(  32|  public void printlnInfos\(String pPrefix\) {) s
(  33|    printInfos\(pPrefix\);) s
(  34|    System.out.print\("\\n"\);) s
(  35|  } ) s
(  36|  ) s
(  37|  ) s
