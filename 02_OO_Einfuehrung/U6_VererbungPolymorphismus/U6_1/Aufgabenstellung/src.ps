(  21|  public double fahren\(double pX, double pY\) {) s
(  22|    System.out.print\("Fahrzeug.fahren\(\): "\);) s
(  23|    double xDiff = mPosX - pX;) s
(  24|    double yDiff = mPosY - pY;) s
(  25|    double km = Math.sqrt\(xDiff * xDiff + yDiff * yDiff\);) s
(  26|    mPosX = pX;) s
